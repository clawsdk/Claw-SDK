  },
  "keywords": [
    "openclaw",
    "ai",
    "agent",
    "sdk",
    "automation",
    "ai-agent",
    "machine-learning",
    "llm"
  ],
  "author": "ClawSDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawsdk/Claw-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/clawsdk/Claw-SDK/issues"
  },
  "homepage": "https://github.com/clawsdk/Claw-SDK#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.9.3"
  }
}
